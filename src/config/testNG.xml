<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Nop Commerce">
    <listeners>
        <listener class-name="Report.Listener"/>
    </listeners>
    <test name="Verify user is logged in">
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name="test.TestVerifyUserIsAbleToGoToDashboard"/>
        </classes>
    </test>
    <test name="Verify user is able to click on book option">
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name="test.VerifyBookPage"/>
        </classes>
    </test>
</suite>